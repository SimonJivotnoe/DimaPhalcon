doctype html
html
  head(lang='en')
    meta(http-equiv='content-type', content='text/html;charset=utf-8')
    title Дима
    include includes/css
    link(rel='shortcut icon', type='image/x-icon', href='favicon.ico')
    | {{ templates }}
  body
    #waitSpinner
    #kimSpinner
    #leftTabsSpinner
    #startPageWrapper.col-md-12
      .col-md-9.col-md-offset-3(style='margin-top: 18%;')
        #runPreferences.col-md-6.mainMenuIcon.hvr-float-shadow.btn-primary
          span.glyphicon.glyphicon-cog(aria-hidden='true')
          h4 Найстройки
        #runDB.col-md-6.mainMenuIcon.hvr-float-shadow.btn-warning
          span.glyphicon.glyphicon-folder-open(aria-hidden='true')
          h4 База Данных
        #runPR.col-md-6.mainMenuIcon.hvr-float-shadow.btn-success
          span.glyphicon.glyphicon-check(aria-hidden='true')
          h4 Создать заказ
    nav#topIconsWrapper.navbar.navbar-default.navbar-fixed-top(style='min-height: 46px;')
      .container(style='margin-top: 4px;')
        #menuIconsTop.col-md-12
          include includes/menuIconsTop
          form#kimIcons.form-inline
            #addKimIcon.form-group.hvr-pulse-grow.defaultIcon
              span.glyphicon.glyphicon-plus.centerIcon(aria-hidden='true')
            #editKimIcon.form-group.hvr-pulse-grow.defaultIcon
              span.glyphicon.glyphicon-pencil.centerIcon(aria-hidden='true')
            #deleteKimIcon.form-group.hvr-pulse-grow.defaultIcon
              span.glyphicon.glyphicon-minus.centerIcon(aria-hidden='true')
            #backKimIcon.form-group.defaultIcon
              span.glyphicon.glyphicon-share-alt.centerIcon(aria-hidden='true')
          .clearer
    #sectionContent
    #outBodyElements
    #backLayout.clearer

    | {{ categoryModal }}

    script(src='js/jquery/jquery-1.11.2.min.js')
    script(src='js/jquery/jquery-ui.min.js')
    link(rel='stylesheet', href='css/bootstrap/bootstrap-table.css')
    link(rel='stylesheet', href='libs/bootstrap/css/bootstrap-theme.min.css')
    link(rel='stylesheet', href='css/font-awesome.css')
    script(src='js/bootstrap/bootstrap.min.js')
    script(src='js/bootstrap/bootstrap-table.js')
    script(src='js/jquery/js/numeral.min.js')
    script(src='js/jquery/jquery-calx-2.2.3.min.js')
    script(src='js/jquery/jquery.caret.js')
    script(src='js/jquery/jquery.mousewheel.js')
    script(src='js/jquery/bootstrap-confirmation.js')
    script(src='js/jquery/pdfmake/pdfmake.min.js')
    script(src='js/jquery/pdfmake/vfs_fonts.js')
    script(src='js/jquery/store+json2.min.js')
    script(src='js/jquery/jquery.resizableColumns.min.js')
    script(src='js/jquery/colorpicker/dist/js/bootstrap-colorpicker.min.js')
    script(src='js/jquery/spin.min.js')
    script(src='js/jquery/tree.jquery.js')
    script(src='js/libs/focus-element-overlay.min.js')
    script(src='js/libs/mustache.min.js')
    | {{ this.assets.outputJs('jsFooter') }}
    script(src='js/libs/jquery.noty.packaged.min.js')
    script(src='js/libs/jquery.notificationcenter.js')
